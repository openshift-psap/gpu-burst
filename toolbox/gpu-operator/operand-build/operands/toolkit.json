{
  "build_operand_git_repo": "https://gitlab.com/nvidia/container-toolkit/container-config.git",
  "build_operand_git_ref": "master",
  "build_operand_namespace": "gpu-operator-resources",
  "build_operand_imagestream_name": "container-toolkit",
  "build_operand_tag": "latest",
  "build_operand_dockerfile": "docker/Dockerfile.ubi8",
  "build_operand_docker_context": ".",
  "build_operand_docker_build_args": [
    {
      "name": "GOLANG_VERSION",
      "value": "1.16.4"
    },
    {
      "name": "VERSION",
      "value": "1.5.0"
    },
    {
      "name": "LIBNVIDIA_CONTAINER_VERSION",
      "value": "1.4.0"
    },
    {
      "name": "NVIDIA_CONTAINER_TOOLKIT_VERSION",
      "value": "1.5.0"
    },
    {
      "name": "NVIDIA_CONTAINER_RUNTIME_VERSION",
      "value": "3.5.0"
    }
  ],
  "build_operand_extra_docker_strategy": {
    "from": {
      "kind": "DockerImage",
      "name": "nvcr.io/nvidia/cuda:11.2.1-base-ubi8"
    }
  }
}