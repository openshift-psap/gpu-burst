{
  "build_operand_git_repo": "https://gitlab.com/nvidia/kubernetes/gpu-operator.git",
  "build_operand_git_ref": "master",
  "build_operand_namespace": "gpu-operator-resources",
  "build_operand_imagestream_name": "gpu-operator-validator",
  "build_operand_tag": "latest",
  "build_operand_dockerfile": "validator/Dockerfile",
  "build_operand_docker_context": ".",
  "build_operand_docker_build_args": [
    {
      "name": "VERSION",
      "value": "v0.1.0"
    },
    {
      "name": "GOLANG_VERSION",
      "value": "1.15"
    },
    {
      "name": "BASE_IMAGE",
      "value": "nvcr.io/nvidia/cuda:11.2.1-base-ubi8"
    },
    {
      "name": "CUDA_SAMPLE_IMAGE",
      "value": "nvcr.io/nvidia/k8s/cuda-sample:vectoradd-cuda11.2.1-ubi8"
    }
  ]
}